{
  "app": "python ./deploy/app.py",
  "context": {
   "@aws-cdk/aws-apigateway:usagePlanKeyOrderInsensitiveId": false,
    "@aws-cdk/aws-cloudfront:defaultSecurityPolicyTLSv1.2_2021": false,
    "@aws-cdk/aws-rds:lowercaseDbIdentifier": false,
    "@aws-cdk/core:stackRelativeExports": false,
    "git_branch": "GIT_BRANCH_NAME",
    "git_release": "BOOLEAN_MANAGE_GIT_RELEASE|DEFAULT=false",
    "tooling_region": "TOOLING_REGION",
    "tooling_vpc_id": "IMPORT_AN_EXISTING_VPC_FROM_TOOLING",
    "resource_prefix": "PREFIX_FOR_ALL_RESOURCES_CREATED_BY_THIS_APP|DEFAULT=dataall",
    "quality_gate": "BOOLEAN_MANAGE_QUALITY_GATE_STAGE|DEFAULT=true",
    "DeploymentEnvironments": [
      {
        "envname": "ENVIRONMENT_NAME",
        "account": "DEPLOYMENT_ACCOUNT",
        "region": "DEPLOYMENT_REGION",
        "with_approval": "BOOLEAN_ADD_CODEPIPELINE_APPROVAL_STEP",
        "vpc_id": "DEPLOY_WITHIN_AN_EXISTING_VPC",
        "internet_facing": "CLOUDFRONT_IF_TRUE_ELSE_ECS_BEHIND_INTERNAL_ALB|DEFAULT=true",
        "ip_ranges": "IP_RANGES_TO_ALLOW_IF_NOT_INTERNET_FACING",
        "apig_vpce": "USE_AN_EXISTING_VPCE_FOR_APIG_IF_NOT_INTERNET_FACING",
        "custom_domain": {
          "hosted_zone_name": "ROUTE_53_EXISTING_DOMAIN_NAME",
          "hosted_zone_id": "ROUTE_53_EXISTING_HOSTED_ZONE_ID"
        },
        "prod_sizing": "SET_INFRA_SIZING_TO_PROD_VALUES_IF_TRUE_ELSE_SMALL_SIZING|DEFAULT=true",
        "enable_cw_rum":  "SET_CLOUDWATCH_RUM_APP_MONITOR|DEFAULT=false",
        "enable_cw_canaries": "SET_CLOUDWATCH_CANARIES_FOR_FRONTEND_TESTING|DEFAULT=false"
      }
    ]
  }
}
